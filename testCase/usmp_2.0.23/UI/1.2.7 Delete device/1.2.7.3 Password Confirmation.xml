<?xml version="1.0" encoding="UTF-8"?>
<Test xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.mydlink.com/testcase.xsd">
	<testCaseName>Password Confirmation</testCaseName>
	<annotation>
		<description>1.2.7.3 Password Confirmation.xml</description>
		<created author="dayu.lei" date="2011-06-17" />
		<lastUpdated author="dayu.lei" date="2011-06-17" />
		<recordInfo deviceIndex="1" account="register" />
	</annotation>
	<TestCase>
		<Function command="setLanguage" target="" value=""></Function>
		
		<!-- login begin -->
		<Html command="open" target="xml(global).pagePath.pagePath" value=""></Html>

		<Verify expected="xml(global).message.titleBar" type="contains">
			<Html command="getValue" target="xml(global).xpath.titleBar"
				value="" />
		</Verify>
		<Html command="type" target="xml(global).xpath.emailTextbox"
			value="account(register).email"></Html>
		<Html command="type" target="xml(global).xpath.passwordTextbox"
			value="account(register).pwd"></Html>
		<Html command="clickAndWait" target="xml(global).xpath.signinDivButton"
			value=""></Html>
		<Verify expected="xml(device).message.titleBar" type="contains">
			<Html command="getValue" target="xml(device).xpath.titleBar"
				value="" />
		</Verify>
		<!-- login end -->
		
		<Html command="waitForElementDisplay" target="xml(device).xpath.stateMsgBar"
			value="60"></Html>
		<Html command="clickAndWait" target="xml(device).xpath.DeleteDeviceBtn"
			value=""></Html>
		<Verify expected="xml(device.unbind1).message.titleBar" type="contains">
			<Html command="getValue" target="xml(device.unbind1).xpath.titleBar"
				value="" />
		</Verify>
		<Html command="check" target="xml(device.unbind1).xpath.selectDeviceOne"
			value=""></Html>
		<Html command="clickAndWait" target="xml(device.unbind1).xpath.nextButton"
			value=""></Html>
		<Html command="click" target="xml(device.unbind2).xpath.nextButton"
			value=""></Html>
		<Html command="waitForElementDisplay" target="xml(device.unbind2).xpath.PasswordVerificationNeededTitle"
			value="60"></Html>	
		<Verify expected="xml(device.unbind2).message.PasswordVerificationNeededTitle">
			<Html command="getValue" target="xml(device.unbind2).xpath.PasswordVerificationNeededTitle"
				value="" />
		</Verify>	
		<Html command="waitForElementDisplay" target="xml(device.unbind2).xpath.PasswordText"
			value="60"></Html>
		<Verify expected="xml(device.unbind2).message.PasswordText">
			<Html command="getValue" target="xml(device.unbind2).xpath.PasswordText"
				value="" />
		</Verify>	
		<Verify expected="xml(device.unbind2).message.PleaseEnterText">
			<Html command="getValue" target="xml(device.unbind2).xpath.PleaseEnterText"
				value="" />
		</Verify>	
			
		<!-- 检查按钮颜色，背景，值 -->
		<!-- color -->	
		<Verify expected="xml(commonInfo).color.white">
			<Html command="getValue" target="xml(device.unbind2).js.PasswordNextButtonFontColor"
				value="" />
		</Verify>	
		<Verify expected="xml(commonInfo).color.blue">
			<Html command="getValue" target="xml(device.unbind2).js.PasswordCancelButtonFontColor"
				value="" />
		</Verify>
		
		<!-- bg -->
		<Verify expected="xml(device.unbind2).image.PasswordCancelButtonBGColor" type="contains">
			<Html command="getValue" target="xml(device.unbind2).js.PasswordCancelButtonBGColor"
				value="" />
		</Verify>
		<Verify expected="xml(device.unbind2).image.PasswordNextButtonBGColor" type="contains">
			<Html command="getValue" target="xml(device.unbind2).js.PasswordNextButtonBGColor"
				value="" />
		</Verify>
		
		<!-- value -->
		<Verify expected="xml(device.unbind2).message.PasswordNextButton" type="contains">
			<Html command="getValue" target="xml(device.unbind2).xpath.PasswordNextButton"
				value="" />
		</Verify>
		<Verify expected="xml(device.unbind2).message.PasswordCancelButton" type="contains">
			<Html command="getValue" target="xml(device.unbind2).xpath.PasswordCancelButton"
				value="" />
		</Verify>
		<!-- 检查按钮颜色，背景，值 结束 -->
		
		<Html command="type" target="xml(device.unbind2).xpath.PasswordInput" value="errorpsw"></Html>
		<Html command="click" target="xml(device.unbind2).xpath.PasswordNextButton"
			value=""></Html>
		<Html command="waitForElementDisplay" target="xml(device.unbind2).xpath.WarningText"
			value="60"></Html>	
		<Verify expected="xml(device.unbind2).message.WarningText">
			<Html command="getValue" target="xml(device.unbind2).xpath.WarningText"
				value="" />
		</Verify>
		
	</TestCase>
</Test>