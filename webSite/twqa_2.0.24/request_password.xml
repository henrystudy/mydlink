<?xml version="1.0" encoding="UTF-8"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.mydlink.com/website.xsd">
	<targets>
		<pagePath name="pagePath">/request_password</pagePath>
		<pagePath name="pagePath">/passmail_send</pagePath>
		
		<!-- logo -->
		<xpath name="logoImg">//div[@id='header']/table/tbody/tr/td/a/img/@src</xpath>
		
		<xpath name="titleBar">//div[@class='jeff2']</xpath>
		<xpath name="Enteryour">//div[@class='jeff3s']/p[1]</xpath>
		<xpath name="emailText">//div[@class='jeff3s']//tr[1]/td[1]</xpath>
		<xpath name="emailTextbox">email<!-- //input[@id='email'] --></xpath>
		<xpath name="send">//div[@class='btn floatl']</xpath>
		<xpath name="back">//div[@class='btnw floatl']</xpath>
		<xpath name="warning">//td[@id='errMsg']</xpath>
		
		<xpath name="Newpsent">//div[@class='jeff2']</xpath>
		<xpath name="confirm">//div[@class='jeff3s']/P[1]</xpath>
		<xpath name="sentEmail">//div[@class='jeff3s']//tr[1]/td[1]</xpath>
		<xpath name="finish">//div[@class='btnw floatl']</xpath>
		
		<!-- JS -->
		<js name="errMsgColorJS">window.jQuery('#errMsg').css('color')</js>
		
		
	</targets>
	<messages>
		<message name="titleBar">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Forgot your password?</content>
		</message>
		<message name="Enteryour">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Enter your mydlink signin e-mail address below and your account password will be e-mailed to you.</content>
		</message>
		<message name="emailText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">E-mail:</content>
		</message>
		<message name="send">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Send</content>
		</message>
		<message name="back">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Back</content>
		</message>
		
		<message name="warning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">This e-mail address is invalid. Please try again.</content>
		</message>
		<message name="Newpsent">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">New password sent successfully.</content>
		</message>
		<message name="confirm">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">An e-mail containing your new password has been sent to your e-mail address.</content>
		</message>
		<message name="sentEmail">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Send to E-Mail Address:</content>
		</message>
		<message name="finish">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Finish!</content>
		</message>
	
		
	</messages>
	<images>
		<image name="logoImg">/images/ucp/dlife_logo.gif</image>
	</images>
</info>
