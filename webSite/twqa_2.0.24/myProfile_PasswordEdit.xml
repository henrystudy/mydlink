<?xml version="1.0" encoding="UTF-8"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.mydlink.com/website.xsd">
	<targets>
		<!-- path -->
		<pagePath name="pagePath">/profile/password</pagePath>

		<!-- Xpath -->
		<xpath name="TitleBar">//div[@class='content']/div[@class='jeff1']/div[@class='jeff2']/span</xpath>

		<xpath name="ChangePasswordText">//form[@id='change_password']/table/tbody/tr[1]/td[1]</xpath>
		<xpath name="LableOldPassword">//form[@id='change_password']/table/tbody/tr[2]/td[1]</xpath>
		<xpath name="LableNewPassword">//form[@id='change_password']/table/tbody/tr[3]/td[1]</xpath>
		<xpath name="LableRetypePassword">//form[@id='change_password']/table/tbody/tr[4]/td[1]</xpath>
		
		<xpath name="InputOldPassword">//input[@id='c_old_password']</xpath>
		<xpath name="InputNewPassword">//input[@id='c_new_password']</xpath>
		<xpath name="InputRetypePassword">//input[@id='c_new_password_verify']</xpath>
		
		<xpath name="SaveBtn">//div[@class='updown20']/div[@class='btn floatl']</xpath>
		<xpath name="BackBtn">//div[@class='jeff4']/div[@class='jeff5']/div[@class='btnbar']/div[@class='btnw floatl']/div</xpath>
		
		<xpath name="LableOldPasswordWarning">//form[@id='change_password']/table/tbody/tr[2]/td[2]/div[@class='warning']</xpath>
		<xpath name="LableNewPasswordWarning">//form[@id='change_password']/table/tbody/tr[3]/td[2]/div[@class='warning']</xpath>
		<xpath name="LableRetypeWarning">//form[@id='change_password']/table/tbody/tr[4]/td[2]/div[@class='warning']</xpath>
		<xpath name="LableUpdateWarning">//div[@class='jeff5']/div[@class='btnbar']/div[@class='warning']</xpath>

		<!-- JS -->
		<js name="errMsgColorJS">window.jQuery('div.warning').css('color')</js>
	</targets>

	<messages>
		<message name="ChangePasswordText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">We have reset your password. Please provide a new one.</content>
		</message>


		<message name="TitleBar">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Please change your password</content>
		</message>

		<message name="LableOldPassword">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Old password:</content>
		</message>

		<message name="LableNewPassword">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">New password:</content>
		</message>
		
		<message name="LableRetypePassword">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Retype password:</content>
		</message>

		<message name="SaveBtn">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Save</content>
		</message>

		<message name="BackBtn">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Back to My Profile</content>
		</message>

		<message name="LableOldPasswordWarning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">The password is incorrect. Please try again.</content>
		</message>
				<message name="LableNewPasswordWarning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Password must have at least 6 characters.</content>
		</message>
				<message name="LableRetypeWarning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Password do not match. Please try again.</content>
		</message>
				<message name="LableUpdateWarning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">The password has been updated.</content>
		</message>
	</messages>
	
</info>