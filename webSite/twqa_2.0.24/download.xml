	<downloadItems>		
		<downloadDevice name="download">
			<xpaths>
				<nameXpath>//div[@class='whitebox']/div/table/tbody/tr[#$$#]/td[#$1$#]//td[@class='hilite']</nameXpath>
				<itemsXpath>//div[@class='whitebox']/div/table/tbody/tr[#$$#]/td[#$1$#]//ul</itemsXpath>
				<deviceCountXpath>//div[@class='whitebox']//tbody//tbody//tr//img</deviceCountXpath>
			</xpaths>
			<deviceNames>
				<deviceName>DeviceName1</deviceName>
				<deviceName>DeviceName2</deviceName>
