<?xml version="1.0" encoding="UTF-8"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.mydlink.com/website.xsd">
	<targets>
		
		<pagePath name="pagePath">/device/unbind3</pagePath>
		<xpath name="titleBar">//div[@class='content']//div[@class='jeff2']</xpath>
		<xpath name="finishButton">//div[@class='btnbar']/div</xpath>
		<xpath name="selectDeviceText">//td[@class='steps']/table/tbody/tr[1]/td</xpath>
		<xpath name="confirmText">//td[@class='steps']/table/tbody/tr[2]/td</xpath>
		<xpath name="completeText">//td[@class='steps']/table/tbody/tr[3]/td</xpath>
		<xpath name="warning">//div[@class='warning']</xpath>
		<xpath name="Thefollowing">//td[@class='contents']/p[1]</xpath>
		
		
		<xpath name="DeviceImg">//div[@class='productbox']//td[1]/img/@src</xpath>
		<xpath name="ModelName">//div[@class='productbxox']//td[2]</xpath>
		<xpath name="mydlinkNo">//div[@class='productbox']//td[3]</xpath>
		<xpath name="DeviceName">//div[@class='productbox']//td[4]</xpath>
		
		<js name="stepOnBg">window.jQuery('td.step_on_bg').css('backgroundImage')</js>
		
		<js name="stepOnColor">window.jQuery('div.step_on').css('color')</js>
		
		<js name="stepOnNumBg">window.jQuery('span.stepn').css('backgroundImage')</js>
		
		<js name="stepOffNumBg">window.jQuery('td.step span.stepn').css('backgroundImage')</js>
		
		<js name="stepNumColor">window.jQuery('span.stepn').css('color')</js>
		<js name="stepOffColor">window.jQuery('td.step').css('color')</js>
		
		<js name="warning">window.jQuery('div.warning').css('color')</js>

	</targets>
	<messages>
		<message name="titleBar">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Delete Devices</content>
		</message>
		<message name="Thefollowing">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">The following devices have been successfully deleted from your mydlink account:</content>
		</message>
		<message name="warning">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">To add the device to a mydlink account and enable remote access to it, please run the device's Setup Wizard again from its included CD.</content>
		</message>
		<message name="finishButton">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Finish</content>
		</message>
		<message name="selectDeviceText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">1Select Device</content>
		</message>
		<message name="confirmText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">2Confirm</content>
		</message>
		<message name="completeText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">3Complete</content>
		</message>
	</messages>
	<images>
		<image name="DeviceImg">/images/products/DCS930L_delete.gif</image>
		<image name="stepOnBg">url(https://twqa.mydlink.com/images/newgraphics/vtab_on.gif)</image>
		<image name="stepOnNumBg">url(https://twqa.mydlink.com/images/newgraphics/i_on.gif)</image>
		<image name="stepOffNumBg">url(https://twqa.mydlink.com/images/newgraphics/i_off.gif)</image>
	</images>
</info>