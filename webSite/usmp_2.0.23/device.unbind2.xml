	<messages>
		<message name="WarningText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">The password is incorrect. Please try again.</content>
		</message>
		<message name="PasswordNextButton">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Next ></content>
		</message>
		<message name="PasswordCancelButton">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Cancel</content>
		</message>
		<message name="PleaseEnterText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">Please enter your mydlink password for verification:</content>
		</message>
		<message name="PasswordText">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
