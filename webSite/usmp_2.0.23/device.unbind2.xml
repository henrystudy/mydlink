		<js name="PasswordNextButtonFontColor">window.jQuery('div.btn').css('color')</js>
		<js name="PasswordNextButtonBGColor">window.jQuery('div.btn').css('background-image')</js>
		<js name="PasswordCancelButtonFontColor">window.jQuery('div.btnw').css('color')</js>
		<js name="PasswordCancelButtonBGColor">window.jQuery('div.btnw').css('background-image')</js>
		<js name="confirmColor">window.jQuery('div.step_on').css('color')</js>
		<js name="completeColor">window.jQuery('td.step').css('color')</js>
		<js name="confirmBGIMG">window.jQuery('div.step span.stepn').css('background-image')</js>
		<js name="completeBGIMG">window.jQuery('div.step span.stepn').css('background-image')</js>
		<js name="completeOffBGIMG">window.jQuery('td.step span.stepn').css('background-image')</js>
		<js name="confirmOffBGIMG">window.jQuery('td.step span.stepn').css('background-image')</js>
		<js name="2CompleteBGIMG">window.jQuery('td.step span.stepn').css('background-image')</js>
		<js name="2ConfirmBGIMG">window.jQuery('div.step_on span.stepn').css('background-image')</js>

