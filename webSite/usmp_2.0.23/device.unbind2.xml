			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
			<content language="en">1Select Device</content>
		</message>
		<message name="Confirm">
			<content language="zh_CN"></content>
			<content language="zh_TW"></content>
			<content language="fr"></content>
